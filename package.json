{
  "name": "medcontracthub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --passWithNoTests",
    "test:unit": "jest --testPathPattern='(/__tests__/.*\\.test\\.(ts|tsx))$' --testPathIgnorePatterns='e2e'",
    "test:api": "jest --testPathPattern='api' --runInBand",
    "test:components": "jest --testPathPattern='components'",
    "test:utils": "jest --testPathPattern='(utils|lib)' --passWithNoTests",
    "test:fast": "jest --bail --maxWorkers=4",
    "test:changed": "jest -o",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:unit && npm run test:e2e",
    "db:reset": "npx supabase db reset",
    "db:types": "npx supabase gen types typescript --local > types/database.types.ts",
    "db:seed": "tsx scripts/seed.ts",
    "analyze": "ANALYZE=true next build",
    "bench": "node scripts/benchmark.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@react-email/components": "^0.0.41",
    "@react-email/render": "^1.1.2",
    "@react-pdf/renderer": "^4.3.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.9",
    "@tanstack/react-query": "^5.79.0",
    "@tanstack/react-query-devtools": "^5.79.0",
    "@types/dompurify": "^3.0.5",
    "@types/react-window": "^1.8.8",
    "@types/react-window-infinite-loader": "^1.0.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "isomorphic-dompurify": "^2.25.0",
    "lucide-react": "^0.511.0",
    "next": "^14.2.29",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-email": "^4.0.15",
    "react-hook-form": "^7.57.0",
    "react-is": "^19.1.0",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "recharts": "^2.15.3",
    "resend": "^4.5.1",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.0",
    "xlsx": "^0.18.5",
    "zod": "^3.24.1",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@swc/jest": "^0.2.38",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.57",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "14.2.18",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.4",
    "typescript": "^5"
  }
}
