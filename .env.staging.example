# Staging Environment Variables
# Copy this to .env.staging and fill in your staging values

# Supabase Staging Project
NEXT_PUBLIC_SUPABASE_URL=https://your-staging-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-staging-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-staging-service-role-key

# API Keys (can use same as production for now)
ANTHROPIC_API_KEY=your-anthropic-api-key
RESEND_API_KEY=your-resend-api-key
SAM_GOV_API_KEY=your-sam-gov-api-key
MISTRAL_API_KEY=your-mistral-api-key
BRAVE_SEARCH_API_KEY=your-brave-search-api-key

# Staging-specific settings
NEXT_PUBLIC_APP_URL=https://staging-medcontracthub.vercel.app
NEXT_PUBLIC_ENVIRONMENT=staging

# Feature flags for staging
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_SENTRY=true

# Redis (will be added later)
REDIS_URL=redis://localhost:6379

# Google OAuth (staging app)
GOOGLE_CLIENT_ID=your-staging-google-client-id
GOOGLE_CLIENT_SECRET=your-staging-google-client-secret