# Staging Environment Variables
# Copy this to .env.staging and fill in values

# Use staging database
DATABASE_URL=postgresql://user:pass@staging-db.supabase.co:5432/postgres
DIRECT_URL=postgresql://user:pass@staging-db.supabase.co:5432/postgres

# Staging Supabase project
NEXT_PUBLIC_SUPABASE_URL=https://staging-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=staging-anon-key
SUPABASE_SERVICE_ROLE_KEY=staging-service-key

# Staging URL
NEXTAUTH_URL=https://staging.medcontracthub.com
NEXT_PUBLIC_APP_URL=https://staging.medcontracthub.com

# Use test Stripe keys
STRIPE_SECRET_KEY=sk_test_...
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_...

# Staging Redis
REDIS_URL=redis://staging-redis.upstash.io

# Environment
NODE_ENV=staging